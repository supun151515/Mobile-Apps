{
  "name": "@progress/kendo-ooxml",
  "description": "Kendo UI OOXML formatting library",
  "author": "Progress",
  "license": "SEE LICENSE IN LICENSE.md",
  "version": "1.3.0",
  "main": "dist/npm/main.js",
  "module": "dist/es/main.js",
  "jsnext:main": "dist/es/main.js",
  "typings": "dist/npm/main.d.ts",
  "es2015": "dist/es2015/main.js",
  "jsdelivr": "dist/cdn/js/kendo-ooxml.js",
  "unpkg": "dist/cdn/js/kendo-ooxml.js",
  "scripts": {
    "build-package": "gulp build-module && gulp build-cdn",
    "test": "gulp test",
    "e2e": "gulp e2e",
    "lint": "gulp lint",
    "semantic-release": "semantic-release pre && semantic-prerelease publish && semantic-release post"
  },
  "keywords": [
    "Kendo UI"
  ],
  "dependencies": {
    "jszip": "^3.1.0"
  },
  "devDependencies": {
    "@krux/condition-jenkins": "1.0.1",
    "@telerik/eslint-config": "^1.0.0",
    "@progress/kendo-package-tasks": "^3.0.10",
    "@telerik/semantic-prerelease": "^1.0.0",
    "cz-conventional-changelog": "^1.1.5",
    "ghooks": "^1.0.3",
    "gulp": "3.9.0",
    "jquery": "3.2.1",
    "semantic-release": "^6.3.6",
    "validate-commit-msg": "^1.1.1"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    },
    "ghooks": {
      "commit-msg": "validate-commit-msg"
    },
    "validate-commit-msg": {
      "types": [
        "feat",
        "fix",
        "docs",
        "style",
        "refactor",
        "perf",
        "test",
        "chore",
        "revert"
      ],
      "warnOnFail": false,
      "maxSubjectLength": 100
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/telerik/kendo-ooxml.git"
  },
  "release": {
    "debug": false,
    "branchTags": {
      "develop": "dev"
    },
    "fallbackTags": {
      "dev": "latest"
    },
    "analyzeCommits": "@telerik/semantic-prerelease/analyzeCommits",
    "generateNotes": "@telerik/semantic-prerelease/generateNotes",
    "getLastRelease": "@telerik/semantic-prerelease/getLastRelease",
    "verifyConditions": "@telerik/semantic-prerelease/verifyConditions",
    "verifyRelease": "@telerik/semantic-prerelease/verifyRelease"
  }

,"_resolved": "https://registry.npmjs.org/@progress/kendo-ooxml/-/kendo-ooxml-1.3.0.tgz"
,"_integrity": "sha512-cKIVbgOBKRPhEHnJ6Otk0rwbW8FbCkcUaxAF19oiZ7Uv1era9lAEn+9JT5ekaegno6iUm99yTiKAsimsBOD8xw=="
,"_from": "@progress/kendo-ooxml@1.3.0"
}